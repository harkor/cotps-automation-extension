{
  "name": "COTPS Automation",
  "description": "Plugin for automation COTPS",
  "version": "1.0.2",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs"
  ],
  "host_permissions": [
    "https://cotps.com/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [ "defaultOptions.json"],
      "matches": [ "<all_urls>" ]
    }
  ]
}
